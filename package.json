{
  "name": "ido-launchpad-ethereum",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "prepare:sepolia": "mustache config/testnet/sepolia.json subgraph.template.yaml > subgraph.yaml",
    "deploy:sepolia": "yarn prepare:sepolia && yarn codegen && yarn build && graph deploy --node https://api.studio.thegraph.com/deploy/ ido-launchpad-ethereum",
    "create-local": "graph create --node http://localhost:8020/ ido-launchpad-ethereum",
    "remove-local": "graph remove --node http://localhost:8020/ ido-launchpad-ethereum",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 ido-launchpad-ethereum",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.68.5",
    "@graphprotocol/graph-ts": "0.32.0",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0"
  }
}
